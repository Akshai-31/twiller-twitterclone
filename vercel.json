{
  "version": 2,
  "builds": [
    { "src": "server/index.js", "use": "@vercel/node" },
    {
      "src": "twiller/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "server/index.js" },
    { "src": "/(.*)", "dest": "twiller/build/$1" }
  ]
}
