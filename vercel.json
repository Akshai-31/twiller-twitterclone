{
  "version": 2,
  "builds": [
    { "src": "twiller/package.json", "use": "@vercel/static-build", "config": { "distDir": "build" } },
    { "src": "server/api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "server/api/index.js" },
    { "src": "/(.*)", "dest": "twiller/build/$1" }
  ]
}
